@model IEnumerable<Blog>
@{
    Layout = "~/Views/Shared/UserLayout.cshtml";
}

<table>
    <tr>
        <th>ID</th>
        <th>Name</th>
    </tr>
    @foreach (var blog in Model)
    {
        <tr>
            <td>@blog.BlogID</td>
            <td>@blog.BlogTitle</td>
        </tr>
    }

</table>